#!/usr/bin/env ruby
require "fuzzystringmatch"

needle = ARGV.first
guess = nil
guess_distance = 0

jarow = FuzzyStringMatch::JaroWinkler.create(:native)

STDIN.readlines.each do |haystack|
  distance = jarow.getDistance(needle, haystack)

  if distance > guess_distance
    guess = haystack
    guess_distance = distance
  end
end

puts guess
